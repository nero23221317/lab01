void setup()
{
  pinMode(9,1);
}

void loop()
{
  int i;
  delay(200);
  for(i=0;i<3;i++)
  {  
    digitalWrite(9,1);
    delay(100);
    digitalWrite(9,0);
    delay(100);
  }
  for(i=0;i<3;i++)
  {  
    digitalWrite(9,1);
    delay(300);
    digitalWrite(9,0);
    delay(100);
  }
  for(i=0;i<3;i++)
  {  
    digitalWrite(9,1);
    delay(100);
    digitalWrite(9,0);
    delay(100);
  }
}
